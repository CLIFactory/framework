{
    "name": "clifactory/framework",
    "description": "A PHP 8 powered wrapper for Symfony Console.",
    "version": "0.0.1",
    "require": {
        "php": "^8.2",
        "symfony/console": "^v6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.3",
        "psr/log": "^3.0.0"
    },
    "autoload": {
        "files": ["src/Functions.php"],
        "psr-4": {
            "CLIFactory\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
